{
    "compilerOptions": {
        "target": "ES2018",
        "module": "None",
        "lib": [
            "ES2023",
            "ES2023.Array",
            "ES2023.Collection",
            "DOM",
        ],
        "rootDir": "./src",
        "outDir": "./lib",
        "strict": true,
        "noImplicitReturns": true,
        "declaration": true,
        "stripInternal": true,
    },
    "include": [
        "./src/**/*.ts",
    ],
}
