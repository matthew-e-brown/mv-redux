{
    "name": "mv-redux",
    "author": "Matthew Brown <matthew.e.brown.17@gmail.com>",
    "version": "0.1.7",
    "license": "MIT",
    "description": "Matrix/vector library for WebGL, plus some helpers.",
    "repository": {
        "type": "git",
        "url": "https://github.com/matthew-e-brown/mv-redux.git"
    },
    "files": [
        "./src",
        "./lib"
    ],
    "type": "module",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        },
        "./init": {
            "import": "./lib/init/index.js",
            "types": "./lib/init/index.d.ts"
        },
        "./helpers": {
            "import": "./lib/helpers/index.js",
            "types": "./lib/helpers/index.d.ts"
        },
        "./*": {
            "import": "./lib/*.js",
            "types": "./lib/*.d.ts"
        }
    },
    "scripts": {
        "build": "tsc"
    },
    "devDependencies": {
        "typescript": "^5.5.4"
    }
}
